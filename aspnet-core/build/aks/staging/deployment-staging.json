{
	"apiVersion": "extensions/v1beta1",
	"kind": "Deployment",
	"metadata": {
		"name": "caprimuldidae"
	},
	"spec": {
		"replicas": 1,
		"strategy": {
			"type": "RollingUpdate",
			"rollingUpdate": {
				"maxSurge": 1,
				"maxUnavailable": 0
			}
		},
		"minReadySeconds": 0,
		"template": {
			"metadata": {
				"labels": {
					"version" : "stg",
					"app": "caprimuldidae"
				}
			},
			"spec": {
				"containers": [
					{
						"name": "caprimuldidae",
						"imagePullPolicy": "Always",
						"image": "acrcaprimuldidae.azurecr.io/caprimuldidaestg:latest",
						"ports": [
							{
								"name": "caprimuldidae",
								"containerPort": 80
							}
						]
					}
				],
				"imagePullSecrets": [
          {
            "name": "acrcaprimuldidaekey"
          }
        ]
			}
		}
	}
}