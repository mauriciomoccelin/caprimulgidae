{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "sqlserver",
    "labels": {
      "app": "sqlserver"
    }
  },
  "spec": {
    "type": "LoadBalancer",
    "ports": [
      {
        "name": "sqlserver",
        "port": 1433,
        "targetPort": 1433,
        "nodePort": 30001,
        "protocol": "TCP"
      }
    ],
    "selector": {
      "app": "sqlserver"
    }
  }
}